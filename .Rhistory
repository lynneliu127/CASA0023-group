install.packages("RefManageR")
# 创建一个数据框，包含引用信息的各个字段
references <- data.frame(
author = c("John Smith", "Alice Jones"),
title = c("An amazing discovery", "The Handbook of Everything"),
journal = c("Journal of Amazing Discoveries", NA),  # NA 表示书籍没有期刊
year = c(2000, 2010),
volume = c(10, NA),  # NA 表示书籍没有卷号
number = c(2, NA),  # NA 表示书籍没有期号
pages = c("123-135", NA)  # NA 表示书籍没有页码
)
# 将数据框转换为 BibTeX 格式的字符串
bibtex <- paste0(
"@article{smith2000,
author = {", references$author[1], "},
title = {", references$title[1], "},
journal = {", references$journal[1], "},
year = {", references$year[1], "},
volume = {", references$volume[1], "},
number = {", references$number[1], "},
pages = {", references$pages[1], "}
}
@book{jones2010,
author = {", references$author[2], "},
title = {", references$title[2], "},
year = {", references$year[2], "}
}"
)
# 将 BibTeX 字符串写入文件
writeLines(bibtex, "references.bib")
View(references)
View(references)
